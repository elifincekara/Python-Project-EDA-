', title='Yaş Dağılımı', labels={'age_at_award': 'Yaş', 'category': 'Ödül Kategorisi'})\n","fig_age.update_layout(barmode='stack')\n","\n","# Yıl Grafiği\n","fig_year = px.line(nobel_selected, x='year', color='category', title='Yıl Dağılımı', labels={'year': 'Yıl', 'category': 'Ödül Kategorisi'})\n","fig_year.update_layout(barmode='group')\n","\n","# Görselleri yan yana sığdıralım\n","fig = plotly.subplots.make_subplots(rows=1, cols=3, subplot_titles=('Cinsiyet Dağılımı', 'Yaş Dağılımı', 'Yıl Dağılımı'))\n","fig.add_trace(fig_sex['data'][0], row=1, col=1)\n","fig.add_trace(fig_age['data'][0], row=1, col=2)\n","fig.add_trace(fig_year['data'][0], row=1, col=3)\n","\n","# Layout düzenlemeleri\n","fig.update_layout(height=400, width=1200, title_text=\"Medicine Kategorisinde Kazananların Genel Dağılımları\")\n","\n","# Görseli göster\n","fig.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":556},"executionInfo":{"status":"ok","timestamp":1705952089110,"user_tz":-180,"elapsed":559,"user":{"displayName":"Elif İncekara","userId":"05259300054081296789"}},"outputId":"9b7c63d6-1e4a-44de-82a2-183d1edf7cc8","id":"sul0AhOWqVtT"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stderr","text":["<ipython-input-13-c8e28cb12df9>:14: SettingWithCopyWarning:\n","\n","\n","A value is trying to be set on a copy of a slice from a DataFrame.\n","Try using .loc[row_indexer,col_indexer] = value instead\n","\n","See the caveats in the documentation: https://pandas.pydata.org/pandas-docs/stable/user_guide/indexing.html#returning-a-view-versus-a-copy\n","\n"]},{"output_type":"display_data","data":{"text/html":["<html>\n","<head><meta charset=\"utf-8\" /></head>\n","<body>\n","    <div>            <script src=\"https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.5/MathJax.js?config=TeX-AMS-MML_SVG\"></script><script type=\"text/javascript\">if (window.MathJax && window.MathJax.Hub && window.MathJax.Hub.Config) {window.MathJax.Hub.Config({SVG: {font: \"